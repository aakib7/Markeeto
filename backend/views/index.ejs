<body>
  <div class="container">
  <h1><%=title %></h1>
  <% for(var i=0;i<mobiles.length;i++) {%>
    <div class="row">
      <div class="col-md-4">
        <div class="image">
          <!-- <img src="images/upload/cameta.jpg"> -->
        </div>
        <h1 class="model">
          <a href="/<%=mobiles[i]._id %>"> Model: <%=mobiles[i].model %> </a>
        </h1>
        <h2 class="company">Company: <%=mobiles[i].company %></h2>
        <h3 class="color mb">Color: <%=mobiles[i].color %></h3>
        <h3 class="Price mb">Price: <%=mobiles[i].price %> PKR</h3>
        <a href="/cart/<%=mobiles[i]._id %>" class="butn">Add to cart</a>
      </div>
    </div>

    <% } %>
    </div>
  </mobiles.length;i++)>
</body>
